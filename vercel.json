{
  "version": 2,
  "buildCommand": "cd frontend/my-react-app && npm run build",
  "outputDirectory": "frontend/my-react-app/dist",
  "framework": "vite",
  "installCommand": "npm install && cd frontend/my-react-app && npm install",
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/frontend/my-react-app/dist/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}